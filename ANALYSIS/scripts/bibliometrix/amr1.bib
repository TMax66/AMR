@article{ ISI:000457453900001,
Author = {Wareth, Gamal and Neubauer, Heinrich and Sprague, Lisa D.},
Title = {{Acinetobacter baumannii - a neglected pathogen in veterinary and
   environmental health in Germany}},
Journal = {{VETERINARY RESEARCH COMMUNICATIONS}},
Year = {{2018}},
Volume = {{43}},
Number = {{1}},
Pages = {{1-6}},
Month = {{FEB}},
Abstract = {{The emergence and global spread of drug resistant Acinetobacter (A.)
   baumannii is a cause of great concern. The current knowledge on
   antibiotic resistance in A. baumannii from animal origin is mostly based
   on few internationally published case reports, investigations of strain
   collections and several whole genome analyses. This lack of data results
   in a somewhat sketchy picture on how to assess the possible impact of
   drug resistant A. baumannii strains on veterinary and public health in
   Germany. Consequently, there is an urgent need to intensify the
   surveillance of A. baumannii in pet animals, the farm animal population
   and wildlife.}},

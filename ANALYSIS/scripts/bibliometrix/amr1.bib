Author = {Fahim, Karima M. and Ismael, Elshaimaa and Khalefa, Hanan S. and Farag,
   Heba S. and Hamza, Dalia A.},
Title = {{Isolation and characterizailon of E. coli strains causing intramammary
   infections from dairy animals and wild birds}},
Journal = {{INTERNATIONAL JOURNAL OF VETERINARY SCIENCE AND MEDICINE}},

@article{ ISI:000333190700005,
Author = {Shehata, Awad Ali and Sultan, Hesham and Hafez, Hafez M. and Krueger,
   Monika},
Title = {{Safety and Efficacy of a Metabolic Drift Live Attenuated Salmonella
   Gallinarum Vaccine Against Fowl Typhoid}},
Journal = {{AVIAN DISEASES}},

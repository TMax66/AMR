@article{ ISI:000376269300001,
Author = {El-Sayed, A. and El-Shannat, S. and Kamel, M. and Castaneda-Vazquez, M.
   A. and Castaneda-Vazquez, H.},
Title = {{Molecular Epidemiology of Mycobacterium bovis in Humans and Cattle}},
Journal = {{ZOONOSES AND PUBLIC HEALTH}},
Year = {{2016}},
Volume = {{63}},

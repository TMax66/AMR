@article{ ISI:000375816000019,
Author = {Prichula, Janira and Pereira, Rebeca Inhoque and Wachholz, Guilherme
   Raffo and Cardoso, Leonardo Almansa and Correa Tolfo, Neidimar Cezar and
   Santestevan, Naiara Aguiar and Medeiros, Aline Weber and Tavares,
   Mauricio and Frazzon, Jeverson and d'Azevedo, Pedro Alves and Guedes
   Frazzon, Ana Paula},
Title = {{Resistance to antimicrobial agents among enterococci isolated from fecal
   samples of wild marine species in the southern coast of Brazil}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2016}},
Volume = {{105}},
Number = {{1}},
Pages = {{51-57}},
Month = {{APR 15}},
Abstract = {{The purpose of this study was to evaluate species distribution,
   antimicrobial resistance profiles, and presence of resistance genes in
   enterococci isolated from fecal samples of wild marine species,
   including seabirds (n = 12), sea turtles (n = 8), and mammals (n = 3)
   found alive or dead in southern coast of Brazil. Enterococci were
   classified based on phenotypic and genotypic characteristics, tested for
   antibiotic susceptibility, and the presence of tet(S), tet(M), tet(L),
   mrsC, and erm(B) genes by PCR. Enterococcus faecalis and Enterococcus
   faecium were the most common species. Single (37.09\%), double
   (25.80\%), and multiple (16.12\%) antibiotic resistance patterns were
   observed. Resistance to rifampicin occurred most frequently. The msrC,
   tet(M), and/or tet(L) genes were detected in 60.15\%, 73.07\%, and
   23.07\% of the resistant strains, respectively. In conclusion, the
   presence of antibiotic resistant strains in these species could be
   related to food web interactions and aquatic pollutants or linked to
   environmental resistome. (C) 2016 Elsevier Ltd. All rights reserved.}},

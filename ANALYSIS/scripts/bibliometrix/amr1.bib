@article{ ISI:000465131200005,
Author = {Jajere, Saleh Mohammed},
Title = {{A review of Salmonella enterica with particular focus on the
   pathogenicity and virulence factors, host specificity and antimicrobial
   resistance including multidrug resistance}},

@article{ ISI:000166466700005,
Author = {Parveen, S and Hodge, NC and Stall, RE and Farrah, SR and Tamplin, ML},
Title = {{Phenotypic and genotypic characterization of human and nonhuman
   Escherichia coli}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{2}},
Pages = {{379-386}},
Month = {{FEB}},
Abstract = {{Estuarine waters receive fecal pollution from a variety of sources,
   including humans and wildlife. Escherichia coli is one of several fecal
   coliform bacteria that inhabit the intestines of many warm-blooded
   animals that sometime contaminate water. Its presence does not
   specifically implicate human fecal input, therefore it is necessary to
   differentiate contamination sources to accurately assess health risks.
   E. coli were isolated from human sources (HS) and nonhuman sources (NHS)
   in the Apalachicola National Estuarine Research Reserve and analyzed for
   fatty acid methyl ester (FAME), O-serogroup, and pulsed-field gel
   electrophoresis (PFGE) profiles. For FAME and PFGE analyses, there was
   no relationship between profile and isolate source. Human source PFGE
   profiles were less diverse than NHS isolates, and conversely for FAME.
   In contrast, O-serogrouping showed less diversity for HS vs. NHS
   isolates, and the predominant HS O-serogroups differed significantly
   (P<0.01) from those of NHS isolates. (C) 2000 Elsevier Science Ltd. All
   rights reserved.}},

@article{ ISI:000085148800006,
Author = {Adesiyun, A},
Title = {{Antibiograms of thermophilic Campylobacter and Salmonella species
   isolated from stray and pet animals and wildlife on Trinidad}},
Journal = {{VETERINARSKI ARHIV}},
Year = {{1999}},
Volume = {{69}},
Number = {{4}},
Pages = {{229-237}},
Month = {{JUL-AUG}},
Abstract = {{Campylobacter species isolated from wildlife (free-ranging or confined),
   stray and pet animals, and Salmonella spp. recovered from wildlife, were
   tested for their susceptibility to antimicrobial agents. Of the 191
   isolates of Campylobacter tested, comprising 186 and 5 strains of C.
   jejuni and C. coli respectively, 21 (11.0\%) exhibited resistance to one
   or more of the 6 antimicrobial agents tested. Resistance was highest to
   ampicillin (7.9\%) and lowest to gentamicin and chloramphenicol (0.0\%).
   Resistance to antimicrobial agents was highest among Ca,Campylobacter
   isolates from agouti (42.9\%) and stray dogs (26.2\%). Among the 54
   Salmonella isolates tested, 50 (92.6\%) each were resistant to
   tetracycline and streptomycin, 47 (87.0\%) to nalidixic acid, 35
   (64.8\%) to kanamycin and 26 (48.1\%) to neomycin. It was concluded that
   resistance to antimicrobial agents tested was low among Campylobacter
   isolates but comparatively very high among Salmonella isolates.}},

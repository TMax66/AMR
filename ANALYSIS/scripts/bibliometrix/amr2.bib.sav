@article{ ISI:000235380900008,
Author = {Steele, CM and Brown, RN and Botzler, RG},
Title = {{Prevalences of zoonotic bacteria among seabirds in rehabilitation
   centers along the Pacific Coast of California and Washington, USA}},
Journal = {{JOURNAL OF WILDLIFE DISEASES}},
Year = {{2005}},
Volume = {{41}},
Number = {{4}},
Pages = {{735-744}},
Month = {{OCT}},
Abstract = {{Many seabirds are rehabilitated annually wildlife rehabilitation centers
   along the Pacific Coast, USA. Although various strains of zoonotic
   bacteria have been isolated froni seabirds, risks to rehabilitators at
   these centers have not been well documented. From November 2001 through
   January 2003, we determined the prevalence of detectable enteric fauna
   by isolation and characterization of Gram-negative bacteria from cloacal
   swabs taken from 26 common murres (Uria aalge), 49 gulls (Larus spp.),
   and 14 other seabirds treated by, rehabilitators in California and
   Washington (USA). At least 25 bacterial species were identified,
   including multiple strains of Escherichia coli, as well as Enterobacter
   cloacae, Citrobacter freundii, and Klebsiella pneumoniae. Antibiotic
   resistance was found in 13 of 19 bacterial isolates tested, including E.
   coli, K. pneumoniae, Acinetobacter baumanii, and Pseudomonas aeruginosa.
   Potential transfer of these bacteria poses a risk to wildlife
   rehabilitators and to seabirds in these centers, as well as to
   free-ranging birds.}},
Publisher = {{WILDLIFE DISEASE ASSN, INC}},
Address = {{810 EAST 10TH ST, LAWRENCE, KS 66044-8897 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Steele, CM (Reprint Author), ABR Inc, POB 249, Forest Grove, OR 97116 USA.
   Humboldt State Univ, Dept Wildlife, Arcata, CA 95521 USA.}},

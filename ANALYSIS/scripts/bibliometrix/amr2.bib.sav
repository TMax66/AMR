@article{ ISI:000267828200004,
Author = {Cacciuttolo, E. and Rossi, G. and Nardoni, S. and Legrottaglie, R. and
   Mani, P.},
Title = {{Anatomopathological aspects of avian aspergillosis}},
Journal = {{VETERINARY RESEARCH COMMUNICATIONS}},

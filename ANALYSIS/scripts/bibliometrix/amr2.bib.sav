@article{ ISI:000250431600005,
Author = {Borilova, Gabriela and Nebola, Mojmir and Steinhauserova, Iva},
Title = {{Occurrence and antibiotic resistance of Campylobacter spp. isolated from
   pheasants (Phasianus colchicus spp. torquatus)}},
Journal = {{ARCHIV FUR LEBENSMITTELHYGIENE}},
Year = {{2007}},
Volume = {{58}},
Number = {{5}},
Pages = {{183-187}},
Month = {{SEP-OCT}},
Abstract = {{The aim of the present study was to determine the occurrence and
   antibiotic resistance patterns of Campylobacter spp. isolates from
   pheasants from farms with intensive production and from wild pheasants.
   Of the total of 302 samples from farm pheasants 70.2 \% (n = 212) were
   Compylobacter spp. positive. The number of Compylobacter spp. positive
   cases in 200 wild pheasants was 27.5 \% (n = 55). Antibiotic
   susceptibility was detected by the agar dilution method. The isolates
   obtained from farm pheasants (n = 142) showed high level resistance to
   (fluoro)quinolones (ciprofloxacine 54,93 \%, nalidixic acid 54.23 \%),
   macrolides (erythromycin 11.27 \%) and tetracyclines (tetracycline 23.94
   \%). Campylobacter spp. isolates from wild pheasants (n = 30) showed
   significant resistance to (fluoro) quinolones (ciprofloxacin 20.00 \%,
   nalidixic acid 26.67 \%),and macrolides (erythromycin 3.33\%). None of
   the examined isolates was resistant to tetracyclines.}},
